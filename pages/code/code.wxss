.left {
    display: inline;
    float: left;
}

.right {
    display: inline;
    float: right;
}

.clear:after {
    display: block;
    content: '';
    clear: both;
}

.clear {
    zoom: 1;
}
.container.code-list {
  display: flex;
  flex-direction: column;
  padding: 40rpx 0;
}

div.warpper-result-table{
	display: block;
	width: 100%;
}
div.warpper-result-table p.main-title {
	display: block;
    font-size: 26rpx;
    color:#3f3f3f;
    margin-bottom: 40rpx;
    text-align: center;
}



div.warpper-result-table span.icon-title {
    padding: 10rpx 0;
    display: block;
    width: 200rpx;
    font-size: 24rpx;
    letter-spacing: 2rpx;
    border-bottom: 4rpx solid #2f8e19;
}

div.result-table {
	display: block;
    position: relative;
    padding: 0 20rpx;
}

div.icon-cell {
    float: left;
    width: 200rpx;
    border-right: 1rpx solid #ccc;
    text-align: center;
}

div.icon-cell .result-ul{
	display: block;
}

div.icon-cell .icon-list {
    position: relative;
    display: block;
}

#barcode {
    font-size: 24rpx;
}

div.icon-cell .icon-img{
    width: 100rpx;
    height: 100rpx;
    vertical-align: top;
    margin-top: 54rpx;
}

.radio {
    position: absolute;
    display: inline-block;
    width: 10rpx;
    height: 10rpx;
    top: 50%;
    border-radius: 50%;
    border: 1px solid #ccc;
    background: #fff;
    margin-top: -2px;
}

.radio>image {
    width: 100%;
    height: 100%;
}

div.result-dt em.radio {
    right: -5rpx;
}

dl.text-cell em.radio {
    left: -5rpx;
}

dl.text-cell {
	display: block;
    padding-left: 200rpx;
    vertical-align: top;
}

div.text-cell>dl {
    vertical-align: middle;
}

dl.text-cell .result-dd,
dl.text-cell .result-dt {
    position: relative;
    display: block;
    font-size: 0;
}

dl.text-cell .talks {
	position: relative; 
	display: block;
    height: 80rpx;
    line-height: 80rpx;
    margin-left:30rpx;      
    background: #f5f5f5;     
    margin-right: 20rpx;
    color: #989898;
    font-size: 26rpx;
    border:1px solid #eee;
    border-radius: 5rpx;

}

dl.text-cell .talks:before {   
   content:"";   
   position: absolute;   
   right: 100%;   
   top: 35rpx;   
   width: 0;   
   height: 0;   
   border-top: 10rpx solid transparent;   
   border-right: 12rpx solid #f5f5f5;   
   border-bottom: 10rpx solid transparent;   
}

.text-cell .title {
	display: block;
    font-size: 28rpx;
    font-weight: normal;
    padding: 14rpx 0;
    text-indent: 30rpx;
    height: 36rpx;
    line-height: 36rpx;
    color:#3f3f3f;
}

